{
  "name": "express-typescript-api",
  "version": "1.0.0",
  "description": "Express.js API with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "echo \"no test specified\" && exit 0",
    "generate-client-cowswap": "swagger-typescript-api generate -p https://raw.githubusercontent.com/cowprotocol/services/main/crates/orderbook/openapi.yml -o ./src/generated/cowswap -n api.ts",
    "generate-client-nearintents": "swagger-typescript-api generate -p https://1click.chaindefuser.com/docs/v0/openapi.yaml -o ./src/generated/nearintents -n api.ts"
  },
  "author": "Nebula",
  "license": "ISC",
  "dependencies": {
    "@cowprotocol/cow-sdk": "^5.10.3",
    "@defuse-protocol/one-click-sdk-typescript": "^0.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/node": "^20.17.50",
    "swagger-typescript-api": "^13.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
